# Database
DATABASE_URL=postgresql://fdata_user:fdata_pass@localhost:5432/fdata

# Server
PORT=3000
NODE_ENV=development

# SCB API
SCB_API_BASE_URL=https://statistikdatabasen.scb.se/api/v2
SCB_RATE_LIMIT=10

# Cache TTL (seconds)
CACHE_TTL_MEMORY=300
CACHE_TTL_DB=86400

# Lantmäteriet API - OGC Features (Fastighetsindelning)
# För att hämta fastighetsgeometrier
# Hämta credentials från: https://www.lantmateriet.se/sv/Om-Lantmateriet/Samverkan-med-andra/api-portal/
# Prenumerera på: "OGC-Features"
# Scope som krävs: ogc-features:fastighetsindelning.read
LM_CONSUMER_KEY=your_consumer_key_here
LM_CONSUMER_SECRET=your_consumer_secret_here
LM_OGC_URL=https://api.lantmateriet.se/ogc-features/v1/fastighetsindelning

# Lantmäteriet API - Registerbeteckning Direkt
# För att söka fastigheter efter beteckning (namn)
# Prenumerera på: "Registerbeteckning Direkt" i API-portalen (GRATIS)
# Dokumentation: https://geotorget.lantmateriet.se/dokumentation/GEODOK/11/latest.html
REGBET_CONSUMER_KEY=your_consumer_key_here
REGBET_CONSUMER_SECRET=your_consumer_secret_here
REGBET_API_URL=https://api.lantmateriet.se/distribution/produkter/registerbeteckning/v5
